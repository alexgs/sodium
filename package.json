{
  "name": "@philgs/sodium",
  "version": "1.1.0",
  "description": "A simple binding to native authenticated encryption functions from libsodium",
  "keywords": [
    "libsodium",
    "encryption"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6.12.0"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "spawn mocha",
    "build": "babel src -d dist"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.12.0"
          }
        }
      ],
      "flow",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philgs/sodium.git"
  },
  "author": {
    "name": "Phil Gates-Shannon",
    "email": "phil.gs@gmail.com",
    "url": "http://philgs.me"
  },
  "license": "MIT",
  "dependencies": {
    "sodium-encryption": "^1.2.3",
    "sodium-native": "^1.10.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.1"
  }
}
